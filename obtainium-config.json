{
  "app": {
    "versionName": "1.0.0",
    "versionCode": 1,
    "minSdkVersion": 21,
    "targetSdkVersion": 33,
    "architectures": [
      "arm64-v8a",
      "armeabi-v7a",
      "x86",
      "x86_64"
    ]
  },
  "obtainium": {
    "sourceType": "github",
    "versionDetection": {
      "method": "tag",
      "pattern": "v\\d+\\.\\d+\\.\\d+",
      "stripPrefix": "v"
    },
    "releaseAssets": {
      "pattern": "\\.apk$",
      "architectureMapping": {
        "arm64-v8a": "-arm64-v8a-",
        "armeabi-v7a": "-armeabi-v7a-",
        "x86": "-x86-",
        "x86_64": "-x86_64-"
      }
    }
  }
} 